[Unit]
Description=Omnitek Modules

[Service]
Type=oneshot
ExecStart=/usr/bin/omnitek-modules start
ExecStop=/usr/bin/omnitek-modules stop
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
